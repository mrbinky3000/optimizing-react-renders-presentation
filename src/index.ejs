<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title><%= htmlWebpackPlugin.options.title %></title>
    <!-- Styles -->
    <% for (var i in htmlWebpackPlugin.files.css) { %>
      <link href="<%= htmlWebpackPlugin.files.css[i] %>" rel="stylesheet" />
    <% } %>
  </head>
  <body>
    <div id="app"></div>
  </body>
  <!-- App JavaScript -->
  <% for (var j in htmlWebpackPlugin.files.js) { %>
    <script src="<%= htmlWebpackPlugin.files.js[j] %>"></script>
  <% } %>
</html>